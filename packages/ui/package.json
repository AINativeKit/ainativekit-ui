{
  "name": "@ainativekit/ui",
  "version": "0.12.0",
  "description": "React UI component library for ChatGPT Apps SDK - Production-ready components, hooks, and design system",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "import": "./dist/tokens/index.js",
      "require": "./dist/tokens/index.cjs"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "import": "./dist/icons/index.js",
      "require": "./dist/icons/index.cjs"
    },
    "./styles": {
      "import": "./dist/style.css"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "chatgpt",
    "openai",
    "react",
    "ui-library",
    "components",
    "ai",
    "design-system",
    "typescript",
    "chatgpt-apps",
    "openai-sdk",
    "chatgpt-apps-sdk",
    "react-components",
    "ui-components",
    "chatgpt-ui",
    "mcp"
  ],
  "author": "AI Native Kit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AINativeKit/chatgpt-apps-sdk-ui.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/AINativeKit/chatgpt-apps-sdk-ui/issues"
  },
  "homepage": "https://github.com/AINativeKit/chatgpt-apps-sdk-ui#readme",
  "scripts": {
    "build": "node scripts/generate-icon-data.cjs && tsc --emitDeclarationOnly --outDir dist && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:deps": "eslint src --ext ts,tsx",
    "storybook": "node scripts/generate-icon-data.cjs && storybook dev -p 6006",
    "build-storybook": "node scripts/generate-icon-data.cjs && storybook build",
    "storybook:test": "test-storybook",
    "normalize-icons": "node scripts/normalize-icons.cjs",
    "generate:icons": "node scripts/generate-icon-components.cjs",
    "generate:icon-data": "node scripts/generate-icon-data.cjs",
    "generate:schemas": "tsx scripts/generate-schemas.ts",
    "generate:docs": "tsx scripts/generate-docs.ts"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-viewport": "^8.6.14",
    "@storybook/preview-api": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.6.14",
    "ts-morph": "^27.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.6.0",
    "typescript-json-schema": "^0.65.1",
    "vite": "^5.4.0",
    "vitest": "^4.0.3"
  },
  "dependencies": {
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "embla-carousel-wheel-gestures": "^8.1.0"
  }
}
